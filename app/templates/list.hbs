{{#link-to 'lists'}}Lists{{/link-to}}
<div>{{#link-to 'list'}}{{title}}{{/link-to}}</div>
<div>
    {{input type="text" value=itemText placeholder="New List Item" action="addItem"}}
</div>
<ol>
    {{#each item in items}}
        <li>
            {{list-item item=item}}
        </li>
    {{/each}}
</ol>
