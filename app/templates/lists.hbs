<div class='page-header'>
    <h1>Lists</h1>
</div>

{{#each model as |list|}}
    <div class='panel panel-default'>
        <div class='panel-heading'>
            <h2 class='panel-title'>
                {{#link-to 'list' list}}{{list.description}}{{/link-to}}
            </h2>
        </div>
        <div class='panel-body'>
            by {{list.user.displayName}}
        </div>
    </div>
{{/each}}
